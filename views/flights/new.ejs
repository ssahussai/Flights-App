<%- include('../partials/header') %>
        <h2>Enter a New Flight Information</h2>
        <form id="new-form" action="/flights" method="POST">
            <label>Airline:
                <input type="text" name="airline">
            </label><br>
            <label>Flight Number:
                <input type="text" name="flightNo">
                </label><br>
            <label>Departs:
                <input type="datetime-local" name="departs">
            </label><br>
            <input type="submit" value="Add Flight">
        </form>
<%- include('../partials/footer') %>